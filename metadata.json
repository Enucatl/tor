{
  "name": "smash-tor",
  "version": "3.0.0",
  "author": "SMASH",
  "summary": "Installs, configures and manages Tor",
  "license": "GPL-3.0",
  "source": "https://gitlab.com/shared-puppet-modules-group/tor",
  "project_page": "https://gitlab.com/shared-puppet-modules-group/tor",
  "issues_url": "https://gitlab.com/shared-puppet-modules-group/tor/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "9",
        "10"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "7",
        "8"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
       "version_requirement": ">= 5.5.0 < 6.0.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 4.5.1"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 4.2.0"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.24.0"
    }
  ],
  "description": "This module manages Tor, making sure it is installed, running, allows for configuration of relays, hidden services, exit policies, etc. With it, you should be able to manage any type of Tor deployment."
}
